<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Leaderboard_Controller">
   <children>
      <ImageView fitHeight="800.0" fitWidth="720.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../res/leaderboard.png" />
         </image>
      </ImageView>
      <Button fx:id="b_back" layoutX="45.0" layoutY="730.0" mnemonicParsing="false" onAction="#b_back_onAction" text="BACK">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="12.0" />
         </font>
      </Button>
      <TableView fx:id="tv_leaderboard" layoutX="150.0" layoutY="185.0" prefHeight="461.0" prefWidth="424.0">
        <columns>
          <TableColumn fx:id="tc_name" prefWidth="221.0" text="NAME" />
          <TableColumn fx:id="tc_score" minWidth="0.0" prefWidth="201.0" text="SCORE" />
        </columns>
      </TableView>
      <TextField fx:id="tf_name" layoutX="230.0" layoutY="661.0">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="12.0" />
         </font>
      </TextField>
      <Button fx:id="b_enter" layoutX="424.0" layoutY="661.0" mnemonicParsing="false" onAction="#b_enter_onAction" text="ENTER">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="l_score" layoutX="330.0" layoutY="134.0" text="SCORE" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
